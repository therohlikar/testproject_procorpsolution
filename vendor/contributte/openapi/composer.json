{
  "name": "contributte/openapi",
  "description": "Pure OpenAPI implementation for your API applications. Great for documentation.",
  "keywords": [
    "api",
    "openapi",
    "http",
    "rest",
    "nette",
    "swagger"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/openapi",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.1",
    "nette/utils": "^4.0.0"
  },
  "require-dev": {
    "contributte/qa": "^0.4",
    "contributte/tester": "^0.3",
    "contributte/phpstan": "^0.1",
    "tracy/tracy": "^2.10.5",
    "symfony/yaml": "^6.4.0 || ^7.0.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\OpenApi\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
